   OR(
      ISNEW(),
      NOT(ISPICKVAL(PRIORVALUE({!myVariable_current.Status__c}), &quot;Scheduled&quot;))
   ),   
   {!myVariable_current.RecordType.Name} = &quot;YBN Sales Event - IL&quot;,
   BEGINS({!myVariable_current.WhatId}, &quot;001&quot;)
)</expression>
    </formulas>
    <formulas>

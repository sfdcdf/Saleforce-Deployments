    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(  
   ISPICKVAL([Event].Activity_Type__c, &apos;External Customer Activity&apos;),
   ISPICKVAL([Event].Milestone__c, &apos;Demo &amp; Value Presentation&apos;),
   ISPICKVAL([Event].Status__c, &apos;Completed&apos;) ,
   [Event].RecordType.Name = &apos;YBN Sales Event - Corporate&apos; 
) </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(  
   ISPICKVAL({!myVariable_current.Activity_Type__c}, &apos;External Customer Activity&apos;),
   ISPICKVAL({!myVariable_current.Milestone__c}, &apos;Demo &amp; Value Presentation&apos;),
   ISPICKVAL({!myVariable_current.Status__c}, &apos;Completed&apos;) ,
   {!myVariable_current.RecordType.Name} = &apos;YBN Sales Event - Corporate&apos; 
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
   ISPICKVAL([Event].Milestone__c, &quot;Demo &amp; Value Presentation&quot;),
   ISPICKVAL([Event].Status__c, &quot;Scheduled&quot;),
   OR(
      ISNEW(),
      NOT(ISPICKVAL(PRIORVALUE([Event].Status__c), &quot;Scheduled&quot;))
   ),   
   [Event].RecordType.Name = &quot;YBN Sales Event - IL&quot;,
   BEGINS([Event].WhatId, &quot;001&quot;)
) </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_3</name>
        <dataType>Boolean</dataType>
        <expression>AND(
   ISPICKVAL({!myVariable_current.Milestone__c}, &quot;Demo &amp; Value Presentation&quot;),

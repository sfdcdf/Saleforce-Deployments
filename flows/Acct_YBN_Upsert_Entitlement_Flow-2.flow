    <decisions>
        <name>Check_Account_Service_Level</name>
        <label>Check Account Service Level</label>
        <locationX>228</locationX>
        <locationY>55</locationY>
        <defaultConnector>
            <targetReference>Get_SlaProcess_T3</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>SL Tier 3</defaultConnectorLabel>
        <rules>
            <name>SL_Tier_1</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>varAccParentName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Aire Serv Heating &amp; Air Conditioning Master</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varAccParentName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Mr. Appliance</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varAccParentName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Mr. Electric</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varAccParentName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Glass Doctor</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varAccParentName</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Miracle-Ear</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_SlaProcess_T1</targetReference>
            </connector>
            <label>SL Tier 1</label>
        </rules>
        <rules>
            <name>SL_Tier_2</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>varServiceLevel</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Service Level A</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varServiceLevel</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Service Level B</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_SlaProcess_T2</targetReference>
            </connector>
            <label>SL Tier 2</label>
        </rules>
    </decisions>

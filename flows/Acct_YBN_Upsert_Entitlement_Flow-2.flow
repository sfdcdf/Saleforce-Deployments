        <locationX>231</locationX>
        <locationY>517</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Entitlement_Needs_Update</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>objEntitlement</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>objEntitlement.SlaProcessId</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>objSlaProcess.Id</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Entitlement_Values</targetReference>
            </connector>
            <label>Entitlement Needs Update</label>
        </rules>
        <rules>
            <name>No_Entitlement_Found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>objEntitlement</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>New_Entitlement_Record</targetReference>
            </connector>
            <label>No Entitlement Found</label>
        </rules>
    </decisions>

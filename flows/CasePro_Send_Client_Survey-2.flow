    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
   NOT(ISBLANK([Case].AccountId)),
   [Case].RecordType.Name = &quot;YBN Inbound Inquiry&quot;,
   [Case].Issue_Resolved_Survey_Send_Date__c = False,
   ISPICKVAL([Case].Status, &quot;Closed&quot;),   
   OR(
      NOT(ISBLANK([Case].SuppliedEmail)),
      NOT(ISBLANK([Case].Spoke_To_Email__c))
   ),
   NOT(
      AND(
         ISPICKVAL([Case].Account.National_Company_Type__c, &quot;Multi-Location&quot;),
         [Case].Account.RecordType.Name = &quot;YBN Master Account&quot;,
         ISPICKVAL([Case].Account.Type, &quot;Customer&quot;)
      )
   )
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
   NOT(ISBLANK({!myVariable_current.AccountId})),
   {!myVariable_current.RecordType.Name} = &quot;YBN Inbound Inquiry&quot;,
   {!myVariable_current.Issue_Resolved_Survey_Send_Date__c} = False,
   ISPICKVAL({!myVariable_current.Status}, &quot;Closed&quot;),   
   OR(
      NOT(ISBLANK({!myVariable_current.SuppliedEmail})),
      NOT(ISBLANK({!myVariable_current.Spoke_To_Email__c}))
   ),
   NOT(
      AND(

            </value>
        </filters>
        <object>Fulfillment__c</object>
        <outputReference>lstFulfills</outputReference>
        <queriedFields>Opportunity__c</queriedFields>
        <queriedFields>Account__c</queriedFields>
        <queriedFields>Status__c</queriedFields>
        <queriedFields>Products_Sold__c</queriedFields>
    </recordLookups>
    <recordLookups>
        <name>Opp_Record</name>
        <label>Opp Record</label>
        <locationX>149</locationX>

                <elementReference>varOppId</elementReference>
            </value>
        </filters>
        <object>Opportunity</object>
        <outputReference>objOpp</outputReference>
        <queriedFields>Id</queriedFields>
        <queriedFields>AccountId</queriedFields>
        <queriedFields>StageName</queriedFields>
        <queriedFields>Products__c</queriedFields>
        <queriedFields>Strategic_Project_Manager__c</queriedFields>
    </recordLookups>
    <recordUpdates>
        <name>Update_New_List</name>
        <label>Update New List</label>
        <locationX>153</locationX>

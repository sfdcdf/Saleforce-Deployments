    <description>Updates the related fulfillment record when stage is updated</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(   
   OR(
      ISPICKVAL([Opportunity].StageName, &quot;Contract&quot;),  
      ISPICKVAL([Opportunity].StageName, &quot;Closed Won&quot;),
      ISPICKVAL([Opportunity].StageName, &quot;Closed Lost&quot;)
   ),

   OR(
      [Opportunity].RecordType.Name = &quot;YBN - Growth Opportunity&quot;,
      [Opportunity].RecordType.Name = &quot;YBN - Initial Transaction&quot;,
      [Opportunity].RecordType.Name = &quot;YBN - Renewal&quot;
   )
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(   
   OR(
      ISPICKVAL({!myVariable_current.StageName}, &quot;Contract&quot;),  
      ISPICKVAL({!myVariable_current.StageName}, &quot;Closed Won&quot;),
      ISPICKVAL({!myVariable_current.StageName}, &quot;Closed Lost&quot;)
   ),

   OR(
      {!myVariable_current.RecordType.Name} = &quot;YBN - Growth Opportunity&quot;,
      {!myVariable_current.RecordType.Name} = &quot;YBN - Initial Transaction&quot;,
      {!myVariable_current.RecordType.Name} = &quot;YBN - Renewal&quot;
   )
)</expression>
    </formulas>

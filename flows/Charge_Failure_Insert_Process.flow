        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
isnew(),
[Case].AccountId  &lt;&gt; &quot;&quot;,
[Case].RecordTypeId = &quot;012600000009UJQAA2&quot;,
$Profile.Id &lt;&gt; &apos;00e32000000ovPU&apos;, /* API Integration Read/Write */
$Profile.Id &lt;&gt; &apos;00e60000000nQe9&apos;, /* System Administrator */
ISPICKVAL([Case].Reason , &quot;Charge Failure&quot;),

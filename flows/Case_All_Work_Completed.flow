    <description>Checks to determine if the Parent Case should be updated to reflect the status that All Work has been completed and the Client needs to be contacted</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
     NOT(ISBLANK([Case].AccountId)),
     NOT(ISBLANK([Case].RecordTypeId)),
     NOT(ISNEW()),
     ISCHANGED([Case].Status),
     [Case].IsClosed,
     NOT(ISBLANK([Case].ParentId)), 
     NOT($Profile.Name = &apos;API Integration Read/Write&apos;),

     OR(
          TEXT([Case].Status) &lt;&gt; &apos;Bug Reported&apos;,
          TEXT([Case].Status) &lt;&gt; &apos;Awaiting Customer Response&apos;
    )

) </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
     NOT(ISBLANK({!myVariable_current.AccountId})),
     NOT(ISBLANK({!myVariable_current.RecordTypeId})),
     NOT(ISNEW()),
     ISCHANGED({!myVariable_current.Status}),
     {!myVariable_current.IsClosed},
     NOT(ISBLANK({!myVariable_current.ParentId})), 
     NOT({!$Profile.Name} = &apos;API Integration Read/Write&apos;),

     OR(
          TEXT({!myVariable_current.Status}) &lt;&gt; &apos;Bug Reported&apos;,
          TEXT({!myVariable_current.Status}) &lt;&gt; &apos;Awaiting Customer Response&apos;
    )

)</expression>

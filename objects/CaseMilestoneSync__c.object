    <fields>
        <fullName>ResolutionUnderDay_1__c</fullName>
        <externalId>false</externalId>
        <formula>AND(
  CONTAINS(Name, "Resolution"),
  ElapsedTimeInDays__c &lt; 1 /* Less Than 1 Day */
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Resolution Under 1 Day</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ResponseUnderHour_1__c</fullName>
        <externalId>false</externalId>
        <formula>AND(
  CONTAINS(Name, "Response"),
  ElapsedTimeInMins__c  &lt; 60  /* Less Than 1 Hour */
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Response Under 1 Hour</label>
        <trackHistory>false</trackHistory>

    <validationRules>
        <fullName>Call_Answering_After_Hours_Details</fullName>
        <active>true</active>
        <description>If Setup Type is equal to After Hours then we must fill out After Hours Details text field.</description>
        <errorConditionFormula>AND(
ISPICKVAL(Setup_Type__c, "After Hours"),
ISBLANK( After_Hours_Details__c) 
)</errorConditionFormula>
        <errorDisplayField>After_Hours_Details__c</errorDisplayField>
        <errorMessage>Please provide hours that the call answering service should be active.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Call_Answering_Change_Requests</fullName>
        <active>true</active>
        <description>Must change the status if changing any of the call answering fields</description>
        <errorConditionFormula>and(
not(ischanged( Call_Answering_Status__c )),
or(
ischanged(   Business_Name__c ),
ischanged(   List_of_Services__c ),
ischanged(   Emergency_Hours__c ),
ischanged(   Business_Hours_Days_Open__c ),
ischanged(   Business_Address__c ),
ischanged(   Setup_Type__c ),
ischanged(   of_Rings_Before_Answer__c ),
ischanged(   Custom_Greeting_del__c ),
ischanged(   First_Response__c ),
ischanged(   Info_to_Verify_on_the_Call__c ),
ischanged(   Closing_Script__c ),
ischanged(   Change_Request_Details__c ),
ischanged(   Question_Issue__c )))</errorConditionFormula>
        <errorDisplayField>Call_Answering_Status__c</errorDisplayField>
        <errorMessage>You must change the Call Answering status if making changes to the existing setup.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Call_Answering_Client_Phone_or_Email</fullName>
        <active>true</active>
        <description>Client phone number or email address needs to be filled out prior to submission so the client can receive the message for the call.</description>
        <errorConditionFormula>AND(
 ISBLANK(Client_Email__c),
 ISBLANK(Client_Phone_Number__c)
)</errorConditionFormula>
        <errorMessage>Client phone number or email address needs to be filled out prior to submission so the client can receive the message for the call.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Change_Request_Details</fullName>
        <active>true</active>
        <description>You must fill out the Change Request Details section when requesting a change to the call answering setup.</description>
        <errorConditionFormula>AND(
ISPICKVAL( Call_Answering_Status__c ,"Change Requested"),
ISBLANK( Change_Request_Details__c ))</errorConditionFormula>
        <errorMessage>You must fill out the Change Request Details section when requesting a change to the call answering setup.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Client_Must_Have_Sponsored</fullName>
        <active>true</active>
        <description>Clients must have Yodle Ads or Yodle Max to be on Call Answering. Since this account does not have either, they cannot be offered our Call Answering product.</description>
        <errorConditionFormula>AND(
NOT(CONTAINS( Account__r.Product_Type__c ,"SEM")),
NOT(CONTAINS( Account__r.Name ,"Miracle")),
not($UserRole.Name = "CS Operations"))</errorConditionFormula>
        <errorMessage>Clients must have Yodle Ads or Yodle Max to be on Call Answering. Since this account does not have either, they cannot be offered our Call Answering product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Deactivated_status_option_for_admin_only</fullName>
        <active>true</active>
        <description>Only system admins have the ability to select "Deactivated" status for call answering status.</description>
        <errorConditionFormula>AND( 
 ISPICKVAL( Call_Answering_Status__c,"Deactivated"),
 $User.UserRoleId&lt;&gt;"00E60000001J9zD",
$User.UserRoleId &lt;&gt;"00E60000000rjxk",
$User.UserRoleId &lt;&gt;"00E60000001Gham",
$User.UserRoleId &lt;&gt;"00E60000000sSEH"
)</errorConditionFormula>
        <errorMessage>If you wish to have the call answering service deactivated, please select "Cancel Service" in the Call Answering Status field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Number_of_rings_must_be_selected</fullName>
        <active>true</active>
        <description>If # of rings is selected as option from Setup Type need to denote how many rings from # of rings before answer field.</description>
        <errorConditionFormula>AND(
ISPICKVAL(Setup_Type__c, "# of Rings"),
ISPICKVAL(of_Rings_Before_Answer__c,"")
)</errorConditionFormula>
        <errorDisplayField>of_Rings_Before_Answer__c</errorDisplayField>
        <errorMessage>Please choose number of rings</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone_Number_Must_Be_Numbers</fullName>
        <active>true</active>
        <description>Only phone numbers should be entered into the Client Cell Phone fields. Please edit accordingly.</description>
        <errorConditionFormula>IF( LEN(  Client_Phone_Number__c ) &gt;14, true, false)</errorConditionFormula>
        <errorMessage>Only numbers should be entered into the Client Cell Phone fields. Please edit accordingly.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Question_Issue_Details</fullName>
        <active>true</active>
        <description>You must fill out the Questions/Issues section when changing the call answering status to questions/issue.</description>
        <errorConditionFormula>AND(
ISPICKVAL( Call_Answering_Status__c ,"Question/Issue"),
ISBLANK(  Question_Issue__c  ))</errorConditionFormula>
        <errorMessage>You must fill out the Questions/Issues section when changing the call answering status to questions/issue.</errorMessage>
    </validationRules>
</CustomObject>

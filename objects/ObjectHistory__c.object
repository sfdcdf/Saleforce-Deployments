    <fields>
        <fullName>TORCHx_Elapsed_Days__c</fullName>
        <externalId>false</externalId>
        <formula>IF(NOT(ISBLANK(EndDateTime__c)), 
(DATETIMEVALUE(EndDateTime__c ) - DATETIMEVALUE( StartDateTime__c )) - 
( FLOOR ( ( ( (DATETimeVALUE(EndDateTime__c )) - DATETIMEVALUE(StartDateTime__c )) / 7 ) ) * 2 ) + 
CASE(MOD ( DATEVALUE(StartDateTime__c) - DATE( 1900,1,6 ),7), 
0,CASE( MOD ((DATEVALUE(EndDateTime__c)) - DATE( 1900,1,6 ),7),0,1,2 ), 
1,CASE( MOD ((DATEVALUE(EndDateTime__c)) - DATE( 1900,1,6 ),7),0,2,1 ), 
IF(MOD(DATEVALUE(StartDateTime__c) - DATE( 1900,1,6 ),7) - MOD((DATEVALUE(EndDateTime__c)) - DATE( 1900,1,6 ),7) &lt;= 0 ,0, 
IF(MOD((DATEVALUE(EndDateTime__c)) - DATE( 1900,1,6 ),7) = 0,1,-2 ) ) ) 
, 
(NOW()- DATETIMEVALUE( StartDateTime__c )) - 
( FLOOR ( ( ( NOW() - DATETIMEVALUE(StartDateTime__c)) / 7 ) ) * 2 ) + 
CASE(MOD ( DATEVALUE(StartDateTime__c) - DATE( 1900,1,6 ),7), 
0,CASE( MOD (TODAY() - DATE( 1900,1,6 ),7),0,1,2 ), 
1,CASE( MOD (TODAY() - DATE( 1900,1,6 ),7),0,2,1 ), 
IF(MOD(DATEVALUE(StartDateTime__c) - DATE( 1900,1,6 ),7) - MOD(TODAY() - DATE( 1900,1,6 ),7) &lt;= 0 ,0, 
IF(MOD(TODAY() - DATE( 1900,1,6 ),7) = 0,1,-2 ) ) ) 
)</formula>
        <label>TORCHx Elapsed Days</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>TORCHx_Elapsed_Hours__c</fullName>
        <externalId>false</externalId>
        <formula>(IF(NOT(ISBLANK(EndDateTime__c)), 
(DATETIMEVALUE(EndDateTime__c ) - DATETIMEVALUE( StartDateTime__c )) - 
( FLOOR ( ( ( (DATETimeVALUE(EndDateTime__c )) - DATETIMEVALUE(StartDateTime__c )) / 7 ) ) * 2 ) + 
CASE(MOD ( DATEVALUE(StartDateTime__c) - DATE( 1900,1,6 ),7), 
0,CASE( MOD ((DATEVALUE(EndDateTime__c)) - DATE( 1900,1,6 ),7),0,1,2 ), 
1,CASE( MOD ((DATEVALUE(EndDateTime__c)) - DATE( 1900,1,6 ),7),0,2,1 ), 
IF(MOD(DATEVALUE(StartDateTime__c) - DATE( 1900,1,6 ),7) - MOD((DATEVALUE(EndDateTime__c)) - DATE( 1900,1,6 ),7) &lt;= 0 ,0, 
IF(MOD((DATEVALUE(EndDateTime__c)) - DATE( 1900,1,6 ),7) = 0,1,-2 ) ) ) 
, 
(NOW()- DATETIMEVALUE( StartDateTime__c )) - 
( FLOOR ( ( ( NOW() - DATETIMEVALUE(StartDateTime__c)) / 7 ) ) * 2 ) + 
CASE(MOD ( DATEVALUE(StartDateTime__c) - DATE( 1900,1,6 ),7), 
0,CASE( MOD (TODAY() - DATE( 1900,1,6 ),7),0,1,2 ), 
1,CASE( MOD (TODAY() - DATE( 1900,1,6 ),7),0,2,1 ), 
IF(MOD(DATEVALUE(StartDateTime__c) - DATE( 1900,1,6 ),7) - MOD(TODAY() - DATE( 1900,1,6 ),7) &lt;= 0 ,0, 
IF(MOD(TODAY() - DATE( 1900,1,6 ),7) = 0,1,-2 ) ) ) 
))*24


/*https://help.salesforce.com/articleView?id=000001100&amp;type=1*/</formula>
        <label>TORCHx Elapsed Hours</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
